<section class="phx-hero">
  <div><%= @counter_value %></div>
  <%= link "Increment", to: "#", phx_click: "inc", class: "button" %>
  <div>
    <span>Online Users</span>
    <ul>
    <%= for {country, %{metas: metas}} <- @presence_list do %>
      <li><%= country %>: <%= length(metas) %></li>
    <% end %>
    </ul>
  </div>
</section>
<div id="chartdiv" phx-update="ignore"></div>
